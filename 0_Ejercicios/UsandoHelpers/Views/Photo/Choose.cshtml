<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Some</title>
</head>
<body>
    <div>
        @Html.ActionLink("Click here to view photo 1", "Display", new { id = 1 });
    </div>

    <div>
        @Html.ActionLink("Click here to view photo 2", "Display", new { id = 2 });
    </div>

    <div>
        @Html.ActionLink("Click here to view photo 3", "Display", new { id = 3 });
    </div>

    <p>LIstado</p>
    @for (int i = 1; i < 10; i++)
    {
        <div>@Html.ActionLink($"Click here to view photo {@i}", "Display", new { id = @i });</div>
    }


</body>
</html>